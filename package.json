  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!**/src/**/*connect.{js,jsx}",
      "!**/src/index.{js,jsx}"
    ],
    "coverageDirectory": "<rootDir>/tests/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupFramework.js"
    ],
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass|md)$": "identity-obj-proxy",
      "\\.(svg)$": "<rootDir>/tests/__mocks__/svgMock.js"
    }
